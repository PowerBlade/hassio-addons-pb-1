    "80/tcp": 8188
  },
  "ports_description": {
    "80/tcp": "Web UI port"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PGID": "int?",
    "PUID": "int?",
    "TZ": "str?",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "slug": "omni-tools",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "1.0.0"
