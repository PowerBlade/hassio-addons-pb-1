{
  "arch": [
    "aarch64",
    "amd64"
  ],
  "description": "Self-hosted, end-to-end-encrypted photo & video storage (Ente server + MinIO).",
  "devices": [
    "/dev/dri",
    "/dev/dri/renderD128",
    "/dev/vchiq",
    "/dev/video10",
    "/dev/video11",
    "/dev/video12"
  ],
  "environment": {
    "PGID": "0",
    "PUID": "0",
    "TZ": "Europe/Paris"
  },
  "image": "ghcr.io/alexbelgium/ente-{arch}",
  "init": false,
  "map": [
    "addon_config:rw"
  ],
  "name": "Ente",
  "options": {
    "DB_PASSWORD": "ente",
    "DISABLE_WEB_UI": true,
    "MINIO_DATA_LOCATION": "/config/minio-data",
    "MINIO_ROOT_PASSWORD": "minioadmin",
    "MINIO_ROOT_USER": "minioadmin",
    "TZ": "Europe/Paris",
    "USE_EXTERNAL_DB": false
  },
  "panel_icon": "mdi:image-multiple",
  "ports": {
    "3000/tcp": 8300,
    "3200/tcp": 8320,
    "8080/tcp": 8280
  },
  "ports_description": {
    "3000/tcp": "Ente web UI",
    "3200/tcp": "MinIO S3 endpoint",
    "8080/tcp": "Ente API (museum)"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?",
    "DB_DATABASE_NAME": "str?",
    "DB_HOSTNAME": "str?",
    "DB_PASSWORD": "str",
    "DB_PORT": "int?",
    "DB_USERNAME": "str?",
    "DISABLE_WEB_UI": "bool?",
    "MINIO_ROOT_PASSWORD": "str",
    "MINIO_ROOT_USER": "str",
    "TZ": "str?",
    "USE_EXTERNAL_DB": "bool?"
  },
  "slug": "ente",
  "startup": "services",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "1.1.57",
  "video": true,
  "webui": "http://[HOST]:[PORT:3000]"
}
